%Custom commands
\newcommand{\resheading}[1]{\medskip
    \colorbox{mygrey}{\begin{minipage}{\dimexpr\textwidth-2\fboxsep\relax}\small 
                        \textbf{#1 \vphantom{p\^{E}}}
                      \end{minipage}}
                            \par\medskip}
